def loadGameElements(){	var folderRoot = Global_getAddonsFolder() + "/stages/TemplateStage/";		var bg;		Console_WriteLine("creating mainstage element");	bg := new_GameElement(0,30);	Console_WriteLine("creating mainstage anim");	var anim := new_Animation("normal", 1,1,1);	Console_WriteLine("adding mainstage frame");	anim.addFrame(folderRoot + "Images/mainstage.png");	bg.addAnimation(anim);	bg.changeAnimation("normal", FORWARD);	this.addElement(bg);		bg := new_GameElement(0,-70);	bg.addAnimation(new_Animation("normal",1, folderRoot + "Images/plat_middle.png"));	bg.changeAnimation("normal", FORWARD);	this.addElement(bg);		bg := new_GameElement(-110, -2);	bg.addAnimation(new_Animation("normal",1, folderRoot + "Images/plat_side.png"));	bg.changeAnimation("normal", FORWARD);	this.addElement(bg);		bg := new_GameElement(110, -2);	anim := new_Animation("normal",1, folderRoot + "Images/plat_side.png");	anim.mirror(true);	bg.addAnimation(anim);	bg.changeAnimation("normal", FORWARD);	this.addElement(bg);}def loadPlatforms(){	var folderRoot = Global_getAddonsFolder() + "/stages/TemplateStage/";		var plat;		plat := new_Platform(PLATFORMTYPE_NORMAL, 0,112);	plat.addAnimation(new_Animation("normal",1, folderRoot + "Images/main_plat.png"));	plat.changeAnimation("normal", FORWARD);	plat.setVisible(false);	this.addPlatform(plat);		plat := new_Platform(PLATFORMTYPE_GOTHROUGH, -46, -74, 92, 10);	this.addPlatform(plat);		plat := new_Platform(PLATFORMTYPE_GOTHROUGH, -154, -7, 90, 10);	this.addPlatform(plat);		plat := new_Platform(PLATFORMTYPE_GOTHROUGH, 66, -7, 90, 10);	this.addPlatform(plat);}def loadHangPoints(){	var hp;		hp := new_HangPoint(LEFT,-212,68);	this.addHangPoint(hp);		hp := new_HangPoint(RIGHT,204,68);	this.addHangPoint(hp);}def constructor(){	var folderRoot = Global_getAddonsFolder() + "/stages/TemplateStage/";		Console_WriteLine("making spawn points");		this.setSpawnPoint(0, 0,0);	this.setSpawnPoint(1,  115, -70);	this.setSpawnPoint(2, -115, -70);	this.setSpawnPoint(3,	0,-135);	this.setSpawnPoint(4,	0, -35);		Console_WriteLine("setting view borders");	this.bottomViewBorder = 276;	this.topViewBorder = -276;	this.leftViewBorder = -400;	this.rightViewBorder = 400;		Console_WriteLine("setting top border");	this.topBorder = -420;		Console_WriteLine("setting item bounds");	this.setItemBoundaries(-200, -225, 200, 60);		Console_WriteLine("calling loadGameElements");	loadGameElements();	Console_WriteLine("calling loadPlatforms");	loadPlatforms();	Console_WriteLine("calling loadHangPoints");	loadHangPoints();		Console_WriteLine("setting background");	this.setBackground(folderRoot + "Images/bg.jpg");	this.setBackgroundScale(1.45);	this.setBackgroundType(STAGE_BG_FIXED);		//this.setWireframeColor(COLOR_BLUE);	//this.showWireframes(true);		return this;}def destructor(){	//}def Draw(g, gameTime){	base_Draw(g, gameTime);}